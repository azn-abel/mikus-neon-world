{
    "name": "Queen of the City",
    "version": "1.0.0",
    "description": "Music Visualizer for Hatsune Miku Magical Mirai 2023",
    "manifest_version": 2,
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": ".png"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "start_url": "./?mode=pwa",
    "display": "fullscreen",
    "theme_color": "#000000",
    "background_color": "#ffffff"
}
  